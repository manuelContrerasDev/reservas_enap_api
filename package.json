{
  "name": "reservas_enap_api",
  "version": "1.0.0",
  "main": "dist/server.js",
  "_moduleAliases": {
    "@": "dist"
  },
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec ts-node --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "postinstall": "npx prisma generate",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "verify:env": "node scripts/verifyEnv.js",
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@emailjs/nodejs": "^5.0.2",
    "@getbrevo/brevo": "^3.0.1",
    "@prisma/adapter-pg": "^7.1.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-xss-sanitizer": "^2.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@prisma/client": "5.14",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/date-fns": "^2.5.3",
    "@types/express": "^5.0.5",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.19.25",
    "@types/node-fetch": "^2.6.13",
    "@types/pg": "^8.15.6",
    "nodemon": "^3.1.10",
    "prisma": "5.14",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  }
}
